{
  "ReservationDatabase": {
    "ConnectionString": "mongodb://localhost:27017/?tls=true",
    "DatabaseName": "scheduling",
    "CollectionName": "reservations"
  },
  "AWS": {
    "Enabled": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Debug",
      "NonHostConsoleApp.Program": "Debug"
    },
    "Console": {
      "LogLevel": {
        "Default": "Debug",
        "Microsoft": "Debug",
        "Microsoft.AspNetCore": "Debug",
        "Microsoft.Hosting.Lifetime": "Debug",
        "NonHostConsoleApp.Program": "Debug"
      },
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "HH:mm:ss ",
        "UseUtcTimestamp": true,
        "JsonWriterOptions": {
          "Indented": true
        }
      }
    }
  }
}
